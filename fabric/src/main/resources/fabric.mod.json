{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${author}"
  ],
  "contact": {
    "homepage": "https://rhm176.de",
    "issues": "https://github.com/Raik176/undertale-death-screen/issues",
    "sources": "https://github.com/Raik176/undertale-death-screen"
  },
  "license": "${license}",
  "icon": "assets/${id}/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "${group}.${id}.fabric.UndertaleDeathScreenFabric"
    ],
    "modmenu": [
      "${group}.${id}.fabric.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "${id}-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "minecraft": "${minecraft}"
  },
  "suggests": {
    "cloth-config": ">=${cloth_config_version}",
    "modmenu": ">=${modmenu_version}"
  }
}